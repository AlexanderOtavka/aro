let x: Num <- 2.3
let tup: (Num Num Bool) <- (1 + (3 * 2)  x * x  #true())
let (a: Num  b: Num  c: Bool) <- tup

let add_or_multiply: (((Num Num) Bool) => Num) <-
    fn ((first: Num  second: Num)  should_add: Bool) =Num=>
        if should_add then
            first + second
        else
            first * second

add_or_multiply <| ((a b) c)
